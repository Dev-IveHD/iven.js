  "config": {
    "commitizen": {
      "path": "./node_modules/cz-adapter-eslint"
    }
  },
  "dependencies": {
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "got": "^11.0.2",
    "is-docker": "^2.0.0",
    "ms": "^2.1.2",
    "opusscript": "^0.0.7",
    "pretty-ms": "^6.0.1",
    "require.js": "^1.0.0",
    "ytdl-core": "^2.1.0",
    "ytpl": "^0.1.20"
